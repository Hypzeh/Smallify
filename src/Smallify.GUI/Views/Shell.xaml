<Window x:Class="Smallify.GUI.Views.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:constants="clr-namespace:Smallify.Module.Core.Constants;assembly=Smallify.Module.Core"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:Smallify.GUI"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:prism="http://prismlibrary.com/"
        Title="Smallify"
        Width="800"
        Height="150"
        mc:Ignorable="d">

    <Border Background="{DynamicResource MainBackgroundBrush}">
        <ContentControl prism:RegionManager.RegionName="{x:Static constants:RegionNames.PLAYER_REGION}" />
    </Border>
</Window>
